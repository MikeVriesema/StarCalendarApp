<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/main">

	<Button
		android:id="@+id/compass"
		android:layout_width="118dp"
		android:layout_height="wrap_content"
		android:layout_above="@+id/information"
		android:layout_alignParentStart="true"
		android:layout_alignParentLeft="true"
		android:layout_marginStart="5dp"
		android:layout_marginLeft="5dp"
		android:layout_marginEnd="155dp"
		android:layout_marginRight="155dp"
		android:layout_marginBottom="-2dp"
		android:layout_toStartOf="@+id/weather"
		android:layout_toLeftOf="@+id/weather"
		android:background="@drawable/button_corner"
		android:text="@string/compass" />

	<Button
		android:id="@+id/weather"
		android:layout_width="126dp"
		android:layout_height="wrap_content"
		android:layout_above="@+id/myCalendar"
		android:layout_alignParentEnd="true"
		android:layout_alignParentRight="true"
		android:layout_marginEnd="3dp"
		android:layout_marginRight="3dp"
		android:layout_marginBottom="1dp"
		android:background="@drawable/button_corner"
		android:text="@string/weather" />

	<Button
		android:id="@+id/myCalendar"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_above="@+id/information"
		android:layout_alignParentStart="true"
		android:layout_alignParentLeft="true"
		android:layout_marginStart="5dp"
		android:layout_marginLeft="5dp"
		android:layout_marginEnd="165dp"
		android:layout_marginRight="165dp"
		android:layout_marginBottom="-46dp"
		android:layout_toStartOf="@+id/information"
		android:layout_toLeftOf="@+id/information"
		android:background="@drawable/button_corner"
		android:text="@string/myCalendar" />

	<Button
		android:id="@+id/information"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentEnd="true"
		android:layout_alignParentRight="true"
		android:layout_alignParentBottom="true"
		android:layout_marginEnd="5dp"
		android:layout_marginRight="5dp"
		android:layout_marginBottom="0dp"
		android:background="@drawable/button_corner"
		android:text="@string/information" />

	<TextView
		android:id="@+id/starvalue"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerHorizontal="true"
		android:layout_marginTop="120dp"
		android:text="@string/app_name"
		android:textAppearance="@style/TextAppearance.AppCompat.Title"
		android:textColor="@color/white"
		android:textSize="40sp" />


</RelativeLayout>


